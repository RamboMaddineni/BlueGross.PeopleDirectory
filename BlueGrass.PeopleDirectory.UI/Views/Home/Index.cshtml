@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <table id="tbl-people-details" class="table responsive-table highlight">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Sur Name</th>
                <th>Profile Picture</th>
                <th>Mobile Number</th>
                <th>Email Address</th>
                <th>Gender</th>
                <th>City</th>
                <th>CountryName</th>
                <th class="no-sort"></th>
            </tr>
        </thead>
        <tbody id="people-details-body">
        </tbody>
    </table>
</div>




@section Scripts {

    <script src="~/js/site.js" type="text/javascript"></script>
}



